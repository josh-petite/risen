<?xml version="1.0"?>
<configuration>
  
  <appSettings>
    <!-- Database -->
    <add key="ConnectionString" value="server=localhost;database=risen;uid=josh;Pwd=test"/>
    
    <!-- Listener Configuration -->
    <add key="MaxNumberOfConnections" value="3000"/>
    <add key="Port" value="4444"/>

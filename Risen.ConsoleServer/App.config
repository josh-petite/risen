<?xml version="1.0"?>
<configuration>
    <startup> 
        
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/></startup>
  <appSettings>
    <!-- Listener Configuration -->
    <add key="MaxNumberOfConnections" value="3000"/>
    <add key="Port" value="4444"/>
    <add key="MaxAcceptOperations" value="10"/>
    <add key="Backlog" value="100"/>
    <add key="ReceivePrefixLength" value="4"/>
    <add key="SendPrefixLength" value="4"/>
    <add key="MaxSimultaneousClientsThatWereConnected" value="0"/>

    <!-- Logger -->
    <add key="IsLoggerEnabled" value="True"/>
    <add key="LogQueue" value=".\PRIVATE$\LogQueue"/>
  </appSettings>
</configuration>
